{
  "name": "cosmocli",
  "version": "0.3.0",
  "private": false,
  "description": "Cosmos DB CLI",
  "main": "./lib/index.js",
  "bin": {
    "cosmocli": "./lib/index.js"
  },
  "dependencies": {
    "@azure/cosmos": "^3.9.1",
    "@azure/identity": "^1.1.0",
    "@azure/keyvault-secrets": "^4.1.0",
    "@azure/storage-blob": "^12.2.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^5.1.0",
    "convict": "^6.0.0",
    "figlet": "^1.4.0",
    "fs-extra": "^9.0.1",
    "path": "^0.12.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/convict": "^5.2.1",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.0",
    "@types/uuid": "^8.3.0",
    "@zeit/cosmosdb-server": "^0.8.3",
    "azurite": "^3.8.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "uuid": "^8.2.0"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "jest --coverage --coverageReporters=lcov --coverageReporters=cobertura --coverageReporters=text --detectOpenHandles --ci --reporters=default --reporters=jest-junit",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "watch:build": "tsc -w",
    "watch:test": "jest --projects jest-unit.config.js --watch",
    "watch:test-all": "jest --watch"
  },
  "author": "Simon Gottschlag <simon.gottschlag@xenit.se>",
  "license": "MIT",
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
